<Page
   
    x:Class="ProjectProposal.DemoGamePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ProjectProposal"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Loaded="OnLoad">

    <Grid  Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <ProgressBar x:Name="_progressBar" HorizontalAlignment="Left" Height="46" Margin="24,614,0,0" VerticalAlignment="Top" Width="836"/>
        <Canvas x:Name="_canvas" HorizontalAlignment="Left" Height="552" Margin="18,18,0,0" VerticalAlignment="Top" Width="1000">
            <Rectangle x:Name="_mapBackground" Fill="#FFF4F4F5" Height="552" Stroke="Black" Width="4000" Canvas.Left="0" Canvas.Top="-4"/>
            <Rectangle x:Name="testHoothoot" Fill="Brown" Height="82" Canvas.Left="12" Canvas.Top="272" Stroke="Black" Width="74"/>


        </Canvas>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="84" Margin="972,610,0,0" VerticalAlignment="Top" Width="244" Click="onClick"/>




    </Grid>


</Page>

